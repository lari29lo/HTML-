    <script>

var X = 81 ** (1/2);    //raiz quadrada de 81 = 9     pois 9*9 = 81    OU  81(1/2)
document.write(X + "<br>");
var X = 2 ** (1/2);
document.write(X + "<br>");
var X = 64 ** (1/3);   //raiz cubica de 64 = 4        pois 4*4*4 = 64  OU  64(1/3)
document.write(X + "<br>");    
   
   </script>
